<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<xliff version="1.0">
    <file source-language="en" target-language="no" datatype="plaintext" original="messages" product-name="pxa_newsletter_subscription" date="2014-09-24T01:00:11+01:01">
        <header />
        <body>
            <!--Frontend labels:START-->
            <trans-unit id="name">
                <source>Name</source>
                <target>Navn</target>
            </trans-unit>
            <trans-unit id="email">
                <source>Email</source>
                <target>E-post</target>
            </trans-unit>
            <trans-unit id="back_to_website">
                <source>Back to website</source>
                <tareget>Tilbake til nettstedet</tareget>
            </trans-unit>
            <trans-unit id="storage">
                <source>Storage (PID)</source>
            </trans-unit>
            <trans-unit id="accept_terms">
                <source>Accept terms</source>
            </trans-unit>
            <trans-unit id="subscribe">
                <source>Subscribe</source>
                <target>Abonnér</target>
            </trans-unit>
            <trans-unit id="unsubscribe">
                <source>Unsubscribe</source>
                <target>Avslutt abonnementet</target>
            </trans-unit>
            <trans-unit id="subscription.subscribed_noconfirm">
                <source>You have successfully subscribed to the newsletter.</source>
                <tareget>Du abonnerer nå på nyhetsbrevet.</tareget>
            </trans-unit>
            <trans-unit id="subscription.subscribed_confirm">
                <source>You're almost done. You should receive a confirmation mail within the next few minutes with instructions how to confirm your email.</source>
                <target>Du er nesten ferdig. Du vil straks motta en e-postbekreftelse med informasjon om hvordan du bekrefter din e-postadresse.</target>
            </trans-unit>
            <trans-unit id="subscription.subscribe_ok">
                <source>You have successfully subscribed to the newsletter.</source>
                <tareget>Du abonnerer nå på nyhetsbrevet.</tareget>
            </trans-unit>
            <trans-unit id="subscription.unsubscribe_confirm">
                <source>You should receive a confirmation mail within the next few minutes with link to confirm unsubscribe.</source>
                <target>Du er nesten ferdig. Du vil straks motta en e-postbekreftelse med informasjon om hvordan du bekrefter din e-postadresse.</target>
            </trans-unit>
            <trans-unit id="subscription.unsubscribe_success">
                <source>You have been successfully unsubscribed from newsletter.</source>
                <target>Du vil ikke lenger motta vårt nyhetsbrev.</target>
            </trans-unit>
            <!--Frontend labels:END-->

            <!--Email notification labels:START-->
            <trans-unit id="mail.greeting">
                <source>Hi</source>
                <target>Hei</target>
            </trans-unit>

            <!--Admin subscribe notification-->
            <trans-unit id="mail.admin.subscribe.subject">
                <source>New newsletter subscription</source>
            </trans-unit>
            <trans-unit id="mail.admin.subscribe.line1">
                <source>New subscription on a site.</source>
            </trans-unit>
            <trans-unit id="mail.admin.subscribe.line2">
                <source>Subscription information</source>
            </trans-unit>

            <!--Admin unsubscribe notification-->
            <trans-unit id="mail.admin.unsubscribe.subject">
                <source>Newsletter unsubscription</source>
            </trans-unit>
            <trans-unit id="mail.admin.unsubscribe.line1">
                <source>User has unsubscribed.</source>
            </trans-unit>
            <trans-unit id="mail.admin.unsubscribe.line2">
                <source>Subscription information</source>
            </trans-unit>

            <!--Subscriber confirmation-->
            <trans-unit id="mail.subscriber.confirmation_subject">
                <source>Newsletter subscription confirmation</source>
                <target>Bekreftelse av nyhetsbrevpåmelding</target>
            </trans-unit>
            <trans-unit id="mail.subscriber.confirmation_line1">
                <source>Please confirm your email address by clicking on the following link:</source>
                <target>Bekreft e-postadressen din ved å klikke på følgende link:</target>
            </trans-unit>
            <trans-unit id="mail.subscriber.confirmation_line2">
                <source>Should you wish to unsubscribe in the future, follow this link:</source>
                <target>Hvis du ønsker å melde deg av nyhetsbrevet en gang i fremtiden kan du følge denne lenken:</target>
            </trans-unit>

            <!--Subscriber success-->
            <trans-unit id="mail.subscriber.success_subject">
                <source>Your subscription was successful</source>
            </trans-unit>
            <trans-unit id="mail.subscriber.success_line1">
                <source>You have successfully subscribed to the newsletter.</source>
                <target>Du abonnerer nå på nyhetsbrevet.</target>
            </trans-unit>

            <!--Unsubscribe confirmation-->
            <trans-unit id="mail.unsubscribe.confirmation_subject">
                <source>Confirm your newsletter subscription cancellation</source>
            </trans-unit>
            <trans-unit id="mail.unsubscribe.confirmation_line1">
                <source>In order to unsubscribe please click the following link:</source>
            </trans-unit>
            <!--Email notification labels:END-->

            <!--Error messages:START-->
            <trans-unit id="error.unsubscribe.not_found">
                <source>We were unable to find an active subscription</source>
                <target>Det finnes ikke noe abonnement for denne e-postadressen.</target>
            </trans-unit>
            <trans-unit id="error.missing_page_type">
                <source>Ajax page type setting is missing. Check if extension TypoScript is included.</source>
                <target></target>
            </trans-unit>
            <trans-unit id="error.already_subscribed">
                <source>A subscription with this email address already exists.</source>
                <target>Et abonnement med denne epostadressen finnes allerede.</target>
            </trans-unit>
            <trans-unit id="error.waiting_for_confirmation">
                <source>A subscription with this email address already exists, but waiting for confirmation. Please check your email box.</source>
                <target>Et abonnement med denne epostadressen finnes allerede.</target>
            </trans-unit>
            <trans-unit id="error.subscribe_error.already_confirmed">
                <source>The subscription has already been confirmed.</source>
                <target></target>
            </trans-unit>
            <trans-unit id="error.subscribe_error.unknown">
                <source>Sorry, something went wrong. Please contact us.</source>
                <target></target>
            </trans-unit>
            <trans-unit id="error.invalid.email">
                <source>Invalid email address.</source>
                <target>Ugyldig epostadresse.</target>
            </trans-unit>
            <trans-unit id="error.invalid.storage_pid">
                <source>Storage PID is not set</source>
                <target></target>
            </trans-unit>
            <trans-unit id="error.invalid.name">
                <source>Invalid or missing name.</source>
                <target>Ugyldig eller manglende navn.</target>
            </trans-unit>
            <trans-unit id="error.invalid.accept_terms">
                <source>You must accept the terms.</source>
                <target></target>
            </trans-unit>
            <trans-unit id="error.sender_email_invalid">
                <source>Invalid sender email. Check your typoscript settings</source>
                <target></target>
            </trans-unit>
            <!--Error messages:END-->
        </body>
    </file>
